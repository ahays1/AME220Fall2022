#61fa34 = rgb(97, 250, 52)
	r: 6 * 16^1 + 1 * 16^0 = 96 + 1 = 97
	g: f * 16^1 + a * 16^0 = 15 * 16 + 10 = 240 + 10 = 250
	b: 3 * 16^1 + 4 * 16^0 = 48 + 4 = 52

#34c6ab = rgb(52, 198, 171)
	r: 3 * 16^1 + 4 * 16^0 = 48 + 4 = 52
	g: c * 16^1 + 6 * 16^0 = 12 * 16 + 6 = 192 + 6 = 198
	b: a * 16^1 + b * 16^0 = 10 * 16 + 11 = 160 + 11 = 171

#b237fe = rgb(178, 55, 254)
	r: b * 16^1 + 2 * 16^0 = 11 * 16 + 2 = 176 + 2 = 178
	g: 3 * 16^1 + 7 * 16^0 = 48 + 3 = 55
	b: f * 16^1 + e * 16^0 = 15 * 16 + 14 = 240 + 14 = 254

#571ab4 = rgb(87, 26, 180)
	r: 5 * 16^1 + 7 * 16^0 = 80 + 7 = 87
	g: 1 * 16^1 + a * 16^0 = 16 + 10 = 26
	b: b * 16^1 + 4 * 16^0 = 11 * 16 + 4 = 176 + 4 = 180

#ab34c6 = rgb(171, 52, 198)
	r: a * 16^1 + b * 16^0 = 10 * 16 + 11 = 160 + 11 = 171
	g: 3 * 16^1 + 4 * 16^0 = 48 + 4 = 52
	b: c * 16^1 + 6 * 16^0 = 12 * 16 + 6 = 192 + 6 = 198

#feb237 = rgb(254, 178, 55)
	r: f * 16^1 + e * 16^0 = 15 * 16 + 14 = 240 + 14 = 254
	g: b * 16^1 + 2 * 16^0 = 11 * 16 + 2 = 176 + 2 = 178
	b: 3 * 16^1 + 7 * 16^0 = 3 * 16 + 7 = 48 + 7 = 55

#1ab457 = rgb(26, 180, 87)
	r: 1 * 16^1 + a * 16^0 = 1 * 16 + 10 = 16 + 10 = 26
	g: b * 16^1 + 4 * 16^0 = 11 * 16 + 4 = 176 + 4 = 180
	b: 5 * 16^1 + 7 * 16^0 = 80 + 7 = 87

#ab4715 = rgb(171, 71, 21)
	r: a * 16^1 + b * 16^0 = 10 * 16 + 11 = 160 + 11 = 171
	g: 4 * 16^1 + 7 * 16^0 = 64 + 7 = 71
	b: 1 * 16^1 + 5 * 16^0 = 1 * 16 + 5 = 16 + 5 = 21
